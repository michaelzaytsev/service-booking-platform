{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.e2e.json"
      }
    ]
  },
  "setupFiles": ["dotenv-flow/config"],
  "testEnvironment": "node",
  "maxWorkers": 1
}
